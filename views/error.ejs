<% layout('layouts/boilerplate') -%>
<div class="row">
  <div class="col-10 offset-1 alert alert-danger" role="alert">
    <h4 class="alert-heading">Error <%=err.status %>: <%= err.message %></h4>
    <p><%= err.stack%></p>
    <hr />
    <p class="mb-0">
      Go to <a href="/campgrounds" class="alert-link">all campgrounds</a>.
    </p>
  </div>
</div>
